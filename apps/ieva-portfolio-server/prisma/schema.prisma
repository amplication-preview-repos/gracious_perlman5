datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Video {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  thumbnail   String?
  title       String?
  updatedAt   DateTime @updatedAt
  url         String?
}
